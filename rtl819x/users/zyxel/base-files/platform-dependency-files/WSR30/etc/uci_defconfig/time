config system 'main'
	option mode 'NTP'
	option manual_year '2014'
	option manual_mon '01'
	option manual_day '01'
	option manual_hour '00'
	option manual_min '00'
	option manual_sec '00'
	option ntpName 'pool.ntp.org'
	option auto_timezone ''
	option timezone 'GMT-0'
	option tzIndex '23'
	option auto_timezone_enable '1'
	option wanpublicIP ''
	option city '(GMT)'
	option auto_timezone_override '1'

config daylight_saving 'DST'
	option enable '0'
	option start_d '0'
	option start_day '01'
	option end_day '01'
	option end_d '0'
	option start_month '03'
	option start_num '5'
	option end_num '5'
	option start_clock '01'
	option end_clock '01'
	option end_month '10'
